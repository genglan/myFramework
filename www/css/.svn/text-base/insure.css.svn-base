.checkbox input, .checkbox-icon {
	position: relative;
	width: 22px;
	height: 22px;
	display: block;
	border: 0;
	background: transparent;
	cursor: pointer;
	-webkit-appearance: none;
}
.checkbox input:before, .checkbox .checkbox-icon:before {
	border-color: #da340a;
}
.checkbox input:checked:before, .checkbox input:checked + .checkbox-icon:before {
	background: #da340a;
}
.checkbox input:checked::before, .checkbox input:checked + .checkbox-icon::before{
	background: #fff;
}
.checkbox input::after, .checkbox-icon::after{
	-webkit-transition: opacity 0.05s ease-in-out;
	-moz-transition: opacity 0.05s ease-in-out;
	transition: opacity 0.05s ease-in-out;
	-webkit-transform: rotate(-55deg);
	-moz-transform: rotate(-55deg);
	transform: rotate(-55deg);
	position: absolute;
	top: 30%;
	left: 26%;
	display: table;
	width: 12px;
	height: 7px;
	border: 1px solid #d23f15;
	border-top: 0;
	border-right: 0;
	content: ' ';
	opacity: 0;
}
.checkbox input:checked:before, input:checked + .checkbox-icon:before {
	border-width: 1px;
}
