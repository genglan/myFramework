/***********************loading add by genglan 2015-1-24 ***************/
*{
	padding: 0;
	margin: 0;
}
ion-nav-view{
	background:#fff;
}
a{
	text-decoration: none;
}
header.bar.bar-header.bar-stable{
	background: #2A2F37;
}
.bottom-scroll-content{
	bottom: 54px;
}
.go-home-page {
	background: url(../img/home.png) no-repeat;
	background-size: 15px 15px;
	padding-left: 20px;
}
.menu .scroll-content{
	background: #323A46;
}
.item-icon-top{
	padding-top: 35px;
	text-align: center;
}
.item-icon-top .icon{
	top: -14px;
}
.menu-left .list .item{
	border: none;
	font-size: 12px;
}
/*未提交保单*/
.item-icon-top.unsubmit{
	background: url(../img/unsubmit.png) no-repeat;
	background-size: 28px 25px;
	background-position: 36px 9px;
	color: #A9A9A9;
}
.item-icon-top.unsubmit-actived{
	background: url(../img/unsubmit-actived.png) no-repeat;
	background-size: 28px 25px;
	background-position: 36px 9px;	
	color: #fff;
}
/*已提交保单*/
.item-icon-top.submit{
	background: url(../img/submit.png) no-repeat;
	background-size: 28px 25px;
	background-position: 36px 9px;
	color: #A9A9A9;
}
.item-icon-top.submit-actived{
	background: url(../img/submit-actived.png) no-repeat;
	background-size: 28px 25px;
	background-position: 36px 9px;
	color: #fff;	
}
.right-content{
	width:100%;
	height:100%;
}
.bar.bar-header {
	background:#d14014;
}
.bar .title {
	color: #fff;
}
.button-icon.icon:before {
	color: #fff;
}
.bar .title a {
	color: #fff;
	font-size: 12px;
}
.right-data{
	position: absolute;
	left: 31.25%;
	width:68.75%;
	height:100%;
}
.middle-menu {
	position: absolute;
	z-index:999;
	left: 0;
	width: 31.25%;
	height: 100%;
	border-right: 1px #ccc solid;
}
.data-view.img-none{
	width: 100%;
	height: 100%;
	background: #f7f7f7;
	text-align: center;
}
.data-view.img-none img{
	width: 247px;
	height: 52px;
	margin: 42% auto;
	margin-bottom: 58px;
}
.img-none p {
	color: #979797;
	text-align: center;
	margin: 30px auto;
	font-size: 16px;
}
.search{
	height: 44px;
	padding: 7px 10px;
	background: #e8e8ed;
}
.search input{
	width: 92%;
	border-radius: 5px;
	height: 28px;
	line-height: 13px;
	text-align: center;
	font-size: 14px;
	background: url(../img/search.png) no-repeat;
	background-position: left 46% center;
	background-size: 14px;
	background-color: #fff;
	padding-left: 8%;
	color:#8e8e93;
}
.search input[type="search"]:focus{
	background-position: left 10px center;
	text-align: left;
}
.search input[type="search"]:focus + .ion-ios7-close-empty{
	position: absolute;
	top: 86px;
	right: 20px;
	background: url(../img/del.png) no-repeat;
	background-size: 10px;
	width: 10px;
	height: 10px;
}
/*手机*/
@media only screen and (max-width: 640px){
	.middle-menu {
		width: 100%;
		border-right: none;
    }
	.right-data{
    	display: none;
    }
}