<ion-side-menus>
  <ion-pane ion-side-menu-content>
    <ion-nav-view name="menuContent" animation="slide-left-right"></ion-nav-view>
  </ion-pane>

  <ion-side-menu side="left" width="100">
    <header class="bar bar-header bar-stable" ng-click="gohomeFun();" >
      <h1 class="title"><a href="javascript:;"class="go-home-page">退出应用</a></h1>
    </header>
    <ion-content class="has-header">
      <div class="list">
        <a class="item item-icon-top unsubmit" ng-click="clickMenu(false);" ng-class="{'unsubmit-actived':!isSubmit}">
          未提交保单
        </a>
        <a class="item item-icon-top submit" ng-click="clickMenu(true);" ng-class="{'submit-actived':isSubmit}">
          已提交保单
        </a>
      </div>
    </ion-content>
  </ion-side-menu>
</ion-side-menus>